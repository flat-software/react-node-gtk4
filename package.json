{
  "name": "react-node-gtk4",
  "private": true,
  "packageManager": "yarn@4.11.0",
  "workspaces": [
    "packages/*",
    "packages/ts-for-gir/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo build -F @react-node-gtk4/renderer",
    "test": "turbo test -F @react-node-gtk4/renderer",
    "codegen": "turbo codegen -F @react-node-gtk4/renderer",
    "publish": "pnpm publish -F react-node-gtk4"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "turbo": "2.6.1",
    "typescript": "^5.9.3"
  }
}
