{
  "name": "react-node-gtk4",
  "private": true,
  "packageManager": "yarn@4.11.0",
  "workspaces": [
    "packages/*",
    "packages/ts-for-gir/*",
    "examples/*"
  ],
  "scripts": {
    "ts": "tsc --build --watch",
    "build": "tsc --build",
    "rng-codegen:codegen": "yarn workspace @react-node-gtk4/codegen codegen"
  },
  "devDependencies": {
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.3.0",
    "typescript": "5.9.3"
  }
}
