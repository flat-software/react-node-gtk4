{
  "name": "@react-node-gtk4/renderer",
  "version": "2.2.0",
  "description": "React renderer for GTK4 using node-gtk",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "ts": "tsc --build --watch",
    "build": "tsup-node src/index.ts",
    "lint": "eslint src",
    "test": "jest",
    "codegen": "./bin/codegen",
    "girs:copy": "ts-for-gir copy Gtk-4.0 -o ./girs -g /usr/share/gir-1.0 /usr/lib/*/gir-1.0",
    "girs:generate": "ts-for-gir generate Gtk-4.0 -o ./src/generated/girs -g ./girs -e node"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eugeniodepalo/react-native-gtk4.git"
  },
  "keywords": [
    "react",
    "gtk",
    "node-gtk",
    "native"
  ],
  "author": "Eugenio Depalo <eugeniodepalo@gmail.com>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/eugeniodepalo/react-native-gtk4/issues"
  },
  "homepage": "https://github.com/eugeniodepalo/react-native-gtk4",
  "dependencies": {
    "lodash": "4.17.21",
    "react": "^19.2.0",
    "react-reconciler": "^0.33.0",
    "remeda": "2.32.0"
  },
  "peerDependencies": {
    "node-gtk": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-react": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@girs/gtk-4.0": "^4.20.1-4.0.0-beta.38",
    "@react-node-gtk4/codegen": "workspace:*",
    "@ts-for-gir/cli": "4.0.0-beta.38",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@types/node": "24.10.1",
    "@types/react": "19.2.4",
    "@types/react-reconciler": "0.32.3",
    "@typescript-eslint/eslint-plugin": "8.46.4",
    "@typescript-eslint/parser": "8.46.4",
    "babel-jest": "30.2.0",
    "eslint": "9.39.1",
    "eslint-plugin-jest": "29.1.0",
    "eslint-plugin-react": "7.37.5",
    "jest": "30.2.0",
    "prettier": "3.6.2",
    "ts-jest": "29.4.5",
    "ts-jest-resolver": "2.0.1",
    "ts-node": "10.9.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  }
}
