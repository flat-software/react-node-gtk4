{
  "name": "@react-node-gtk4/codegen",
  "version": "1.0.0",
  "license": "MPL-2.0",
  "type": "module",
  "scripts": {
    "ts": "tsc --build --watch",
    "build": "tsc --build",
    "codegen": "./bin/codegen",
    "codegen:girs": "ts-for-gir generate Gtk-4.0 Adw-1 -o ../rng-renderer/src/generated/girs -g ./girs -e node",
    "codegen:widgets": "node --experimental-transform-types ./src/index.ts ./girs ../rng-renderer"
  },
  "dependencies": {
    "@ts-for-gir/lib": "3.3.0",
    "remeda": "2.32.0",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@ts-for-gir/cli": "3.3.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3"
  }
}
